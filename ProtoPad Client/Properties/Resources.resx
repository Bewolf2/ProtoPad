  <data name="ResultHtmlWrap" xml:space="preserve">
    <value>&lt;!DOCTYPE html&gt;
&lt;html&gt;
&lt;head&gt;
	&lt;meta http-equiv='X-UA-Compatible' content='IE=edge;chrome=1'&gt;
	&lt;style&gt;
	*{font-family: Arial; font-size:11px}
	table{ border-collapse: collapse;margin:5px;border: 2px solid #69f;}
	td,th{text-align:left;border:1px solid grey; padding: 2px;vertical-align: top}
	th{background-color:#cde;}
	thead td{background-color:#69f;color:white;font-weight:bold}
	.leftarrow{
		width: 0; 
		height: 0; 
		border-top: 6px solid transparent;
		border-bottom: 6px solid transparent;	
		border-left: 6px solid white;
		text-align: top;
		display: inline-block;
		margin-right: 5px;
		cursor: pointer;
	}
	.leftarrow.unfolded{
		width: 0; 
		height: 0; 
		border-left: 6px solid transparent;
		border-right: 6px solid transparent;	
		border-top: 6px solid white;
	}
	.beyondmaxlevel
	{
	    background-color: #faa;
	}
	img{
		max-width:400px;
	}
	&lt;/style&gt;
    
&lt;/head&gt;
&lt;body&gt;
	&lt;div id='wrapallthethings'&gt;&lt;/div&gt;
	&lt;script src="http://code.jquery.com/jquery-1.9.1.min.js"&gt;&lt;/script&gt;
	&lt;script&gt;
		$(function(){
			$('#wrapallthethings').on('click', '.leftarrow', function()
			{
				var $table = $(this).closest('table');
				var $tablebody = $table.children('tbody');
				if ($tablebody.is(':visible'))
				{
					$tablebody.hide();
					$table.children('thead').find('tr:nth-child(2)').hide();
					$(this).removeClass('unfolded');
				} else
				{
					$tablebody.show();
					$table.children('thead').find('tr:nth-child(2)').show();
					$(this).addClass('unfolded');
				}

			});
		});

	    function Update()
	    {
		    $('table[data-level="2"] tbody, table.collapsed tbody').hide();
		    $('table[data-level="2"] thead tr:nth-child(2), table.collapsed thead tr:nth-child(2)').hide();
		    $('table[data-level="0"] .leftarrow').addClass('unfolded');
		    $('table[data-level="2"] .leftarrow, table.collapsed .leftarrow').removeClass('unfolded');
	    }
	&lt;/script&gt;
&lt;/body&gt;
&lt;/html&gt;</value>
